{
  "name": "azdev-automation",
  "version": "1.0.11",
  "description": "Azure DevOps automation framework enables access control automation of projects, pipelines and repositories configuration in Azure DevOps Services",
  "main": "./automation.js",
  "types": "./automation.d.ts",
  "files": [
    "*.js",
    "*.d.ts",
    "common/*.js",
    "common/*.d.ts",
    "factories/*.js",
    "factories/*.d.ts",
    "helpers/*.js",
    "helpers/*.d.ts",
    "interfaces/*.js",
    "interfaces/*.d.ts",
    "interfaces/common/*.js",
    "interfaces/common/*.d.ts",
    "interfaces/factories/*.js",
    "interfaces/factories/*.d.ts",
    "interfaces/helpers/*.js",
    "interfaces/helpers/*.d.ts",
    "interfaces/mappers/*.js",
    "interfaces/mappers/*.d.ts",
    "interfaces/readers/*.js",
    "interfaces/readers/*.d.ts",
    "interfaces/updaters/*.js",
    "interfaces/updaters/*.d.ts",
    "mappers/*.js",
    "mappers/*.d.ts",
    "readers/*.js",
    "readers/*.d.ts",
    "updaters/*.js",
    "updaters/*.d.ts",
    "schemas/*.json"
  ],
  "scripts": {
    "build": "tsc --build --clean && tsc",
    "lint": "eslint . --ext .ts",
    "test": "mocha -r ts-node/register tests/**/*.test.ts --no-timeouts --reporter mocha-multi-reporters --reporter-options configFile=mocha.json",
    "patch": "npm version patch --no-git-tag-version",
    "clean": "npm prune --production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmitryserbin/azdev-automation"
  },
  "keywords": [
    "Azure",
    "Azure DevOps",
    "Automation",
    "RBAC"
  ],
  "author": "Dmitry Serbin",
  "license": "MIT",
  "dependencies": {
    "@types/debug": "^4.1.7",
    "ajv": "^8.11.0",
    "azure-devops-node-api": "^11.1.1",
    "debug": "^4.3.4",
    "meow": "^10.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "chai": "^4.3.6",
    "eslint": "^8.18.0",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "mocha-multi-reporters": "^1.5.1",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.8.1",
    "typemoq": "^2.1.0",
    "typescript": "^4.7.4"
  }
}
