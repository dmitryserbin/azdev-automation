{
  "name": "azdev-automation",
  "version": "1.0.4",
  "description": "Azure DevOps automation framework enables access control automation of projects, pipelines and repositories configuration in Azure DevOps Services",
  "main": "./automation.js",
  "types": "./automation.d.ts",
  "files": [
    "*.js",
    "*.d.ts",
    "common/[*.js, *.d.ts]",
    "factories/[*.js, *.d.ts]",
    "helpers/*.js, *.d.ts]",
    "interfaces/*.js, *.d.ts]",
    "mappers/*.js, *.d.ts]",
    "readers/*.js, *.d.ts]",
    "updaters/*.js, *.d.ts]",
    "schemas/*.json"
  ],
  "scripts": {
    "build": "tsc --build --clean && tsc",
    "test": "mocha -r ts-node/register tests/**/*.test.ts --no-timeouts --reporter mocha-multi-reporters --reporter-options configFile=mocha.json",
    "patch": "npm version patch --no-git-tag-version",
    "clean": "npm prune --production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmitryserbin/azdev-automation"
  },
  "keywords": [
    "Azure",
    "Azure DevOps",
    "RBAC",
    "Automation"
  ],
  "author": "Dmitry Serbin",
  "license": "MIT",
  "dependencies": {
    "@types/debug": "^4.1.5",
    "ajv": "^6.11.0",
    "azure-devops-node-api": "^10.0.0",
    "debug": "^4.1.1",
    "meow": "^5.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/meow": "^5.0.0",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "@types/sinon-chai": "^3.2.3",
    "chai": "^4.2.0",
    "mocha": "^7.0.1",
    "mocha-junit-reporter": "^1.23.3",
    "mocha-multi-reporters": "^1.1.7",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0",
    "ts-node": "^8.6.2",
    "typemoq": "^2.1.0",
    "typescript": "^3.7.5"
  }
}
